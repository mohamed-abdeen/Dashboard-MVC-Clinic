@model Doctor
@{
    ViewData["Title"] = "Update";
}

<h1>Update New Doctor</h1>

<form asp-action="Edit">
    <input hidden asp-for="Id" />
    <div class="form-group">
        <label class="col-form-label" asp-for="Id"></label>
        <input class="form-control" asp-for="Id" />
        <span asp-validation-for="Id"></span>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input class="form-control" asp-for="Name" />
        <span asp-validation-for="Name"></span>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="IsActive"></label>
        <input class="form-control" asp-for="IsActive" />
        <span asp-validation-for="IsActive"></span>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Email"></label>
        <input class="form-control" asp-for="Email" />
        <span asp-validation-for="Email"></span>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="PhoneNumber"></label>
        <input class="form-control" asp-for="PhoneNumber" />
        <span asp-validation-for="PhoneNumber"></span>
    </div>

    <input type="submit" value="save" class="btn btn-primary" />
    <a asp-action="Index" class="btn btn-light">Back To List</a>



</form>